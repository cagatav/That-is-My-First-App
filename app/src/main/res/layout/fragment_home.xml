        android:layout_marginTop="30dp"
        android:text="5555555555"
        android:textColor="@color/white" />

    <ImageView
        android:id="@+id/icon2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@id/icon1"
        android:layout_centerHorizontal="false"
        android:layout_marginTop="75dp"
        android:src="@drawable/profileico" />

    <TextView
        android:id="@+id/profilePhone2"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_below="@id/profilePhone1"
        android:layout_marginStart="125dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/shapebackground"
        android:padding="20dp"
        android:textColorHint="@color/white"
        app:drawableTint="@color/gold" />

    <ImageView
        android:id="@+id/im31"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@id/profilePhone1"
        android:layout_marginStart="150dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/namexdd" />

    <TextView
        android:id="@+id/part31"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/profilePhone1"
        android:layout_marginStart="180dp"
